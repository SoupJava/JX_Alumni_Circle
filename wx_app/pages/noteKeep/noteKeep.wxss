/* pages/noteKeep/noteKeep.wxss */
page{
  background-color: rgb(0, 172, 132);
}
.allrecord{
  padding: 20rpx;
  display: flex;
  flex-direction: column;
  z-index: 2;
}
.filed{
  font-size: 40rpx !important;
  color: black !important;
  background-color:rgba(255, 255, 255, 0.5) !important;
  border: 0.5rpx outset rgba(1, 1, 1, 0) !important;
  /* outline: 0.5rem solid khaki !important; */
  box-shadow: 0 0 0 2rpx rgb(34, 34, 34) !important;
  border-radius: 12rpx !important;
  /* font: bold 1rem sans-serif !important; */
  /* margin: 2rem !important; */
  /* padding: 1rem !important; */
  /* outline-offset: 0.5rem !important; */
}
.button{
  display: flex;
  flex-direction: row;
  margin-top: 20rpx;
  padding: 10rpx;
}
.buttonst{
  font-size: large !important;
  font-weight: bolder;
  box-shadow: 0 0 0 2rpx rgb(34, 34, 34) !important;
}
.page {
  height: 100%;
  background-color: rgb(0, 172, 132);
  min-height: 100%;
  width: 100%;
  position: absolute;
  z-index: 1;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}
.regist{
  display: flex;
  flex-direction: column;
  flex-grow: 1;
}
.welcome{
  padding: 5%;
  color: #fff;
  font-size: 40rpx;
  font-weight: bold;
}
/* 向下看 */
.welcome .eye0 {
  display: flex;
  flex-direction: row;
  margin-top: 200rpx;
  height: 150rpx;
}

.welcome .eye0 .left {
  display: flex;
  width: 100rpx;
  height: 100rpx;
  border-radius: 100%;
  border-style: solid;
  border-width: 40rpx;
  animation-duration: 6s;
  animation-iteration-count: infinite;
  box-shadow: 1px 1px 1px 1px rgb(233, 233, 233);
}

.welcome .eye0 .right {
  display: flex;
  width: 100rpx;
  height: 100rpx;
  border-radius: 100%;
  margin-left: 80rpx;
  border-style: solid;
  border-width: 40rpx;
  animation-duration: 6s;
  animation-iteration-count: infinite;
  box-shadow: 1px 1px 1px 1px rgb(233, 233, 233);
}

@keyframes myeye0 {
  0% {
    border-left-width: 40rpx;
    border-right-width: 40rpx;
    border-bottom-width: 40rpx;
    border-top-width: 40rpx;
  }

  40% {
    border-left-width: 10rpx;
    border-right-width: 70rpx;
    border-bottom-width: 10rpx;
    border-top-width: 70rpx;
  }

  60% {
    border-left-width: 70rpx;
    border-right-width: 10rpx;
    border-bottom-width: 10rpx;
    border-top-width: 70rpx;
  }

  100% {
    border-left-width: 40rpx;
    border-right-width: 40rpx;
    border-bottom-width: 40rpx;
    border-top-width: 40rpx;
  }
}

