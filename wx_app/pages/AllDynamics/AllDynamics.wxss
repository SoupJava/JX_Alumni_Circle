/* pages/AllDynamics/AllDynamics.wxss */
.page {
  height: 100%;
  /* background-color: rgb(0, 172, 132); */
  min-height: 100%;
  width: 100%;
  position: absolute;
  z-index: 1;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}
.page .top{
  height: 30%;
  min-height: 500rpx;
  background-color: rgb(0, 172, 132);
  display: flex;
  /* flex-direction: column; */
}

.page .control{
  width: 90%;
  display: flex;
  margin-left: 5%;
  margin-top: -50rpx;
  background-color: #fff;
  border-radius: 5%;
  border: 2px solid rgb(184, 184, 184);
  box-shadow: 1px 1px 1px rgba(185, 185, 185,0.5);
  padding-top: 20rpx ;
  padding-bottom: 20rpx ;
}
.page .control .controlitem{
  font-weight: 300;
  font-size: small;
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: center;
  text-align: center;
}
.page .control .controlitem .item{
  width: 100%;
  flex-direction: column;
  flex-grow: 1;
}

/* 问答求助 */

.container1{
  margin: 3%;
  background-color: rgb(255, 255, 255);
  padding: 3%;
  border-radius: 6px;
  border: 2px solid rgb(184, 184, 184);
  box-shadow: 1px 1px 1px rgba(185, 185, 185,0.5);
}
.container-id{
  color: black;
  font-weight: bolder;
}
.userid{
  display: flex;
  flex-direction:row;
  align-items:center;
}
.sj{
  font-size:small;
  color: gray;
}
.detail{
  color:gray;
}

/* 校园动态 */

.moments{
margin: 3%;
}
.documents{
margin-left: 3rem;
}
.photo-view{
margin-bottom: 20rpx;
display: inline-block;
}
.photo{
background: rgb(185, 185, 185);
float: left;
margin-right: 10rpx;
margin-top: 10rpx;
}
.dzpl{
display: flex;
flex-direction: row;
}
.discuss-view{
background: white;
width: 97%;
}

.discuss{
background: rgb(235, 235, 235);
line-height: 35rpx;
}

.discuss label{
font-size: 12px;
}

.discuss-user{
color: rgb(88, 103, 138);
}

/* 二手出售 */
.goodlist{
float: left;
/* width: 50%; */
margin: 10px;
display: flex;
flex-direction: column;
background-color: #fff;
border-radius: 10px;
box-shadow:1px 1px 1px 1px rgb(233, 233, 233);
}
.goodtext{
display: block;
background-color: #fff;
text-overflow:ellipsis; 
word-wrap:break-word;
padding: 5rpx;
}

.welcome{
  padding: 5%;
  color: #fff;
}
/* 向下看 */
.welcome .eye0 {
  display: flex;
  flex-direction: row;
  margin-top: 50rpx;
  height: 150rpx;
}

.welcome .eye0 .left {
  display: flex;
  width: 100rpx;
  height: 100rpx;
  border-radius: 100%;
  border-style: solid;
  border-width: 40rpx;
  animation-duration: 6s;
  animation-iteration-count: infinite;
  box-shadow: 1px 1px 1px 1px rgb(233, 233, 233);
}

.welcome .eye0 .right {
  display: flex;
  width: 100rpx;
  height: 100rpx;
  border-radius: 100%;
  margin-left: 80rpx;
  border-style: solid;
  border-width: 40rpx;
  animation-duration: 6s;
  animation-iteration-count: infinite;
  box-shadow: 1px 1px 1px 1px rgb(233, 233, 233);
}

@keyframes myeye0 {
  0% {
    border-left-width: 40rpx;
    border-right-width: 40rpx;
    border-bottom-width: 40rpx;
    border-top-width: 40rpx;
  }

  40% {
    border-left-width: 10rpx;
    border-right-width: 70rpx;
    border-bottom-width: 10rpx;
    border-top-width: 70rpx;
  }

  60% {
    border-left-width: 70rpx;
    border-right-width: 10rpx;
    border-bottom-width: 10rpx;
    border-top-width: 70rpx;
  }

  100% {
    border-left-width: 40rpx;
    border-right-width: 40rpx;
    border-bottom-width: 40rpx;
    border-top-width: 40rpx;
  }
}

